<?xml version='1.0' encoding='UTF-8'?>
<query xmlns="http://n2oapp.net/framework/config/schema/query-4.0">
    <unique result-mapping="[0]">
        <sql>
            SELECT :select
            FROM integration.participant
            WHERE code = :id
        </sql>
    </unique>

    <fields>
        <field id="participant_code" domain="string">
            <select mapping="['code']"/>
            <filters>
                <eq filter-id="id"/>
            </filters>
        </field>

    </fields>
</query>

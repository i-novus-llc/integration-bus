<?xml version='1.0' encoding='UTF-8'?>
<page xmlns="http://n2oapp.net/framework/config/schema/page-1.0"
      xmlns:wgt="http://n2oapp.net/framework/config/schema/n2o-widget-3.0"
      xmlns:fs="http://n2oapp.net/framework/config/schema/fieldset-2.0"
      xmlns:ctrl="http://n2oapp.net/framework/config/schema/n2o-control-1.0">
    <object-id>registry</object-id>
    <layout>n2o/layout/single</layout>
    <name> </name>

    <regions>
        <region place="single" type="none">
            <container id="main">
                <wgt:form>
                    <wgt:query-id>sys_permission</wgt:query-id>
                    <fs:field-set>
                        <ctrl:hidden id="participant_method_id"/>
                        <ctrl:classifier id="participant" label="Идентификатор участника" required="true" css-class="n2o-label-6">
                            <ctrl:query query-id="participant" value-field-id="id" label-field-id="name">
                                <ctrl:pre-filters>
                                    <ctrl:pre-filter field-id="participant_method_id" ref="participant_method_id"/>
                                </ctrl:pre-filters>
                            </ctrl:query>
                        </ctrl:classifier>
                        <!--<ctrl:classifier id="group" label="Код группы">
                            <ctrl:query query-id="group" label-field-id="name" value-field-id="id"/>
                        </ctrl:classifier>-->
                        <ctrl:input-text id="callback_url" label="URL Ответа" css-class="n2o-label-6"/>
                        <ctrl:checkbox id="sync" label="Асинхронный" default-value="false" css-class="n2o-label-6"/>
                    </fs:field-set>
                </wgt:form>
            </container>
        </region>
    </regions>

</page>


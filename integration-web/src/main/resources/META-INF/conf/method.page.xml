<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-2.0" name="Сервисы (операции)">

    <form id="main" query-id="method" default-values-query-id="method_default" object-id="registry">
        <fields>
            <hidden id="participant_code"/>
            <input-text id="method_code" label="Идентификатор метода" required="true"/>
            <input-text id="url" label="URL" domain="string" required="true"/>
            <checkbox id="disable" label="Приостановлена" default-value="false"/>
        </fields>
    </form>

</simple-page>

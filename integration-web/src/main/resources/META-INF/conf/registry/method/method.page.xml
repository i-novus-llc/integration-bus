<?xml version='1.0' encoding='UTF-8'?>
<simple-page xmlns="http://n2oapp.net/framework/config/schema/page-3.0" name="Регистрация сервиса (операции)">
    <form id="main" query-id="method" default-values-query-id="method_default" object-id="registry">
        <fields>
            <hidden id="participant_code"/>
            <input-text id="method_code" label="Наименование сервиса" required="true"/>
            <input-text id="url" label="URL" domain="string" required="true"/>
            <input-select id="integration_type" label="Тип взаимодействия" required="true" query-id="integrationType"
                          label-field-id="name" search-field-id="id"/>
            <checkbox id="disable" label="Приостановлена" default-value="false"/>
        </fields>
    </form>

</simple-page>

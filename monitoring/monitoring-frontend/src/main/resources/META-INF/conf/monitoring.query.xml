<?xml version='1.0' encoding='UTF-8'?>
<query xmlns:n2o="http://n2oapp.net/framework/config/schema/n2o-query-executions-1.0"
       xmlns="http://n2oapp.net/framework/config/schema/query-3.0">
    <object-id>monitoring</object-id>
    <name>monitoring</name>
    <execution>
        <n2o:sql>
            <n2o:items-query>select :select
                from integration.monitoring m
                where :where
                order by :order
            </n2o:items-query>
            <n2o:count-query>select count(m)
                from integration.monitoring m
                where :where
            </n2o:count-query>
            <n2o:alias>m</n2o:alias>
        </n2o:sql>
    </execution>

    <fields>
        <field>
            <id>id</id>
            <domain>integer</domain>
            <display/>
            <sorting/>
            <filters auto-generate="true"/>
        </field>

        <field>
            <id>uid</id>
            <domain>string</domain>
            <display/>
            <sorting/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>date_time</id>
            <domain>date</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>sender</id>
            <domain>string</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>receiver</id>
            <domain>string</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>operation</id>
            <domain>string</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>status</id>
            <domain>string</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
        <field>
            <id>error</id>
            <domain>string</domain>
            <display/>
            <filters auto-generate="true"/>
        </field>
    </fields>
</query>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.0.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <changeSet id="create_schema_integration" author="asamoilov" dbms="postgresql">
        <sql>
            CREATE SCHEMA integration;
        </sql>
    </changeSet>

    <changeSet id="create_registry_table" author="asamoilov" dbms="postgresql">
        <sql>
            CREATE TABLE integration.registry
            (
            code character varying(50) NOT NULL,
            url character varying(250) NOT NULL,
            host character varying(250) NOT NULL,
            CONSTRAINT registry_pk PRIMARY KEY (code)
            );
        </sql>
    </changeSet>

</databaseChangeLog>